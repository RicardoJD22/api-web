---
interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="es">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Astro description" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
	</head>
	<body class="bg-[#FFFDD2] h-screen">
		<header class="flex justify-around items-center mb-6 pt-14">
			<img class="m-auto w-[200px]" src="/api_logo.png" alt="" />
			<img class="m-auto mt-8 w-[300px]" src="/bcs_logo.png" alt="" />
		</header>
		<slot />
	</body>
</html>
